<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.alissonranielsousa.sinewsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainSobreCurso"
            android:label="@string/title_activity_main_sobre_curso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainPlanoCurso"
            android:label="@string/title_activity_main_plano_curso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainProfessores"
            android:label="@string/title_activity_main_professores"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainOffice"
            android:label="@string/title_activity_main_office"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>